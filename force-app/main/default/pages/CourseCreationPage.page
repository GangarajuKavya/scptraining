<apex:page standardController="Course__c" showHeader="true" sidebar="true" lightningStylesheets="true">
    <apex:form >
        <apex:pageBlock title="Create Course"  >
            <apex:pageBlockSection >
                <apex:inputField label="Course Name" value="{!Course__c.Name}"/>
                <apex:inputField label="Course Code" value="{!Course__c.Course_Code__c}"/>
                <apex:inputField label="Course Start Date" value="{!Course__c.Course_Start_Date__c}"/>
           
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton action="{!save}" value="Save"/>
            </apex:pageBlockButtons>
            
        </apex:pageBlock>
    </apex:form>
  
</apex:page>